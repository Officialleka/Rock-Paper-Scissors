{
  "version": 2,
  "builds": [
    {
      "src": "index/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/scripts.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}